{
  "name": "maze-runner-vr-frontend",
  "version": "1.0.0",
  "description": "Frontend do jogo Maze Runner VR",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "concurrently \"npm run backend\" \"npm run frontend\" --names \"BACKEND,FRONTEND\" --prefix-colors \"cyan,green\" --kill-others-on-fail",
    "backend": "cd ../backend && npm run dev",
    "frontend": "BACKEND_URL=http://localhost:3001 PORT=3000 nodemon server.js",
    "frontend-only": "BACKEND_URL=http://localhost:3001 PORT=3000 nodemon server.js",
    "dev:remote": "BACKEND_URL=https://jogo-s89j.onrender.com nodemon server.js",
    "prod": "cross-env NODE_ENV=production node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "maze",
    "game",
    "vr",
    "multiplayer"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1"
  }
}
